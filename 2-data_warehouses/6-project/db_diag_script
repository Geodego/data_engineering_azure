// Use DBML to define your database structure
// Docs: https://dbml.dbdiagram.io/docs

Table Fact_Trip {
  trip_id varchar [primary key]
  start_station_id varchar
  end_station_id varchar
  rider_id integer
  duration_in_minutes integer
  rider_age integer
  started_at datetime
  ended_at datetime
  date_key integer
  starting_hour integer
}

Table Fact_Payments {
rider_id integer
payment_date datetime
date_key int
payment_amount decimal
}

Table Dim_Rider {
  rider_id integer [primary key]
  adress varchar
  first varchar
  last varchar
  birthday datetime
  is_member bool
  age_at_account_start integer
}

Table Dim_Station {
  station_id varchar
  name varchar
  latitude float
  longitude float
}

Table Dim_Date {
  date_key integer [primary key]
  week_day integer
  month integer
  quarter integer
}

Ref: Dim_Rider.rider_id < Fact_Trip.rider_id // many-to-one

Ref: Dim_Station.station_id < Fact_Trip.start_station_id

Ref: Dim_Station.station_id < Fact_Trip.end_station_id

Ref: Dim_Date.date_key < Fact_Trip.date_key

Ref: Dim_Rider.rider_id < Fact_Payments.rider_id

Ref: Dim_Date.date_key < Fact_Payments.date_key



